{
    "name": "container app",
    "dockerComposeFile": [
        "../docker-compose.yml",
        "../.devcontainer/docker-compose.yml"
    ],
    "service": "go",
    "workspaceFolder": "/workspace/app",
    "shutdownAction": "none",
    "extensions": [
        "golang.go",
        "golang.go-nightly",
    ],
    "settings": { 
        "go.useLanguageServer": true
    },
}